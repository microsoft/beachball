{
  "name": "beachball",
  "version": "2.50.1",
  "description": "The Sunniest Semantic Version Bumper",
  "repository": {
    "type": "git",
    "url": "https://github.com/microsoft/beachball"
  },
  "homepage": "https://microsoft.github.io/beachball/",
  "license": "MIT",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "bin": "./bin/beachball.js",
  "engines": {
    "node": ">=20"
  },
  "files": [
    "bin",
    "lib/!(__*)",
    "lib/!(__*)/**/*"
  ],
  "scripts": {
    "build": "tsc",
    "change": "node ./lib/cli.js change",
    "checkchange": "node ./lib/cli.js check",
    "docs": "vuepress dev docs --host localhost",
    "docs:build": "vuepress build docs",
    "format": "prettier --write '**/*'",
    "lint": "eslint --max-warnings 0",
    "prepare": "husky",
    "release": "node ./lib/cli.js publish -y",
    "release:docs": "yarn docs:build && yarn gh-pages -d docs/.vuepress/dist --dotfiles",
    "start": "tsc -w --preserveWatchOutput",
    "test": "jest",
    "test:all": "yarn test:unit && yarn test:func && yarn test:e2e",
    "test:e2e": "jest --selectProjects e2e",
    "test:func": "jest --selectProjects functional",
    "test:unit": "jest --selectProjects unit",
    "test:watch": "jest --watch",
    "update-snapshots": "yarn test:unit -u && yarn test:func -u && yarn test:e2e -u"
  },
  "lint-staged": {
    "*": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "cosmiconfig": "^8.3.6",
    "execa": "^5.0.0",
    "fs-extra": "^11.1.1",
    "lodash": "^4.17.15",
    "minimatch": "^3.0.4",
    "p-graph": "^1.1.2",
    "p-limit": "^3.0.2",
    "prompts": "^2.4.2",
    "semver": "^7.0.0",
    "toposort": "^2.0.2",
    "workspace-tools": "^0.38.0",
    "yargs-parser": "^21.0.0"
  },
  "devDependencies": {
    "@eslint/compat": "^1.2.3",
    "@eslint/js": "^9.15.0",
    "@jest/globals": "^29.0.0",
    "@types/fs-extra": "^11.0.0",
    "@types/lodash": "^4.14.191",
    "@types/minimatch": "^5.0.0",
    "@types/node": "^20.0.0",
    "@types/prompts": "^2.4.2",
    "@types/semver": "^7.3.13",
    "@types/tmp": "^0.2.3",
    "@types/toposort": "^2.0.3",
    "@types/yargs-parser": "^21.0.0",
    "@verdaccio/types": "^10.8.0",
    "@vuepress/bundler-vite": "^2.0.0-rc",
    "@vuepress/plugin-search": "^2.0.0-rc",
    "@vuepress/theme-default": "^2.0.0-rc",
    "eslint": "^9.15.0",
    "find-free-port": "^2.0.0",
    "get-port": "^5.0.0",
    "gh-pages": "^6.0.0",
    "globals": "^15.12.0",
    "husky": "^9.0.0",
    "jest": "^29.0.0",
    "lint-staged": "^15.0.0",
    "mermaid": "^11.4.0",
    "normalized-tmpdir": "^1.0.1",
    "prettier": "~3.3.0",
    "sass-embedded": "^1.81.0",
    "strip-ansi": "^6.0.1",
    "tmp": "^0.2.1",
    "ts-jest": "^29.0.0",
    "typescript": "~5.7.0",
    "typescript-eslint": "^8.15.0",
    "verdaccio": "^6.0.0",
    "verdaccio-auth-memory": "^10.2.0",
    "verdaccio-memory": "^10.3.0",
    "vuepress": "^2.0.0-rc",
    "vuepress-plugin-md-enhance": "^2.0.0-rc"
  },
  "resolutions": {
    "@types/node@npm:*": "^20.0.0"
  },
  "packageManager": "yarn@4.5.2"
}
