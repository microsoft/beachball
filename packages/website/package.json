{
  "name": "beachball-website",
  "version": "0.0.1",
  "private": true,
  "description": "Beachball website",
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "author": "Ken Chau",
  "main": "n/a",
  "scripts": {
    "build": "yarn clean && gatsby build --prefix-paths",
    "build:local": "gatsby build",
    "clean": "rm -rf public && rm -rf .cache",
    "format:js": "npm run prettier -- --write",
    "lint": "npm run lint:js && npm run lint:md",
    "lint:js": "eslint --ext .js,.jsx --ignore-pattern public,static . && npm run prettier -- --list-different",
    "lint:md": "remark content/posts/",
    "prettier": "prettier --write '{,!(node_modules,.cache)/**/}*.{js,jsx}'",
    "serve": "gatsby serve",
    "start": "gatsby develop"
  },
  "dependencies": {
    "autoprefixer": "^9.7.4",
    "babel-plugin-lodash": "^3.2.11",
    "babel-plugin-macros": "^2.8.0",
    "babel-plugin-styled-components": "^1.10.1",
    "babel-preset-gatsby": "^0.2.27",
    "gatsby": "^2.18.25",
    "gatsby-link": "^2.2.28",
    "gatsby-plugin-catch-links": "^2.1.24",
    "gatsby-plugin-feed": "^2.3.26",
    "gatsby-plugin-google-fonts": "^1.0.1",
    "gatsby-plugin-manifest": "^2.2.37",
    "gatsby-plugin-nprogress": "^2.1.18",
    "gatsby-plugin-offline": "^3.0.32",
    "gatsby-plugin-postcss": "^2.1.19",
    "gatsby-plugin-react-helmet": "^3.1.21",
    "gatsby-plugin-react-svg": "^3.0.0",
    "gatsby-plugin-remove-trailing-slashes": "^2.1.20",
    "gatsby-plugin-sharp": "^2.3.13",
    "gatsby-plugin-sitemap": "^2.2.26",
    "gatsby-plugin-styled-components": "^3.1.17",
    "gatsby-plugin-tailwindcss": "1.0.3",
    "gatsby-remark-autolink-headers": "^2.1.23",
    "gatsby-remark-copy-linked-files": "^2.1.36",
    "gatsby-remark-images": "^3.1.42",
    "gatsby-remark-mermaid": "^1.2.0",
    "gatsby-remark-prismjs": "^3.3.30",
    "gatsby-source-filesystem": "^2.1.46",
    "gatsby-transformer-json": "^2.2.25",
    "gatsby-transformer-remark": "^2.6.48",
    "lodash": "^4.17.4",
    "lodash-webpack-plugin": "^0.11.4",
    "mermaid": "^8.4.5",
    "prismjs": "^1.19.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-helmet": "^5.2.1",
    "react-icons": "^3.8.0",
    "styled-components": "^5.0.0",
    "tailwind.macro": "^1.0.0-alpha.10",
    "tailwindcss": "^1.1.4",
    "tailwindcss-grid": "^1.2.1"
  },
  "devDependencies": {
    "cli-glob": "^0.1.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^17.1.1",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.1.0",
    "prettier": "^2.0.4",
    "remark-cli": "^7.0.1",
    "remark-preset-lint-recommended": "^3.0.1",
    "stylefmt": "^6.0.0"
  },
  "remarkConfig": {
    "plugins": [
      "remark-preset-lint-recommended"
    ]
  }
}
